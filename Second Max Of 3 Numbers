#include <iostream>
using namespace std;
int main()
{
int n;
long long x1,x2,x3;
cin>>n;
while(n--)
{
cin>>x1>>x2>>x3;
if(x1>x2 && x1>x3)
{
if(x2>x3)
cout<<x2<<endl;
else
cout<<x3<<endl;
}
else if(x2>x1 && x2>x3)
{
if(x1>x3)
cout<<x1<<endl;
else
cout<<x3<<endl;
}
//if(x3>x2 && x3>x3)
else
{
if(x1>x2)
cout<<x1<<endl;
else
cout<<x2<<endl;
}
}
return 0;
}
